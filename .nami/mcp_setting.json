{
    "mcpServers": {
        "browser-use": {
            "type": "stdio",
            "command": "uv",
            "args": [
                "--directory",
                "path_to/mcp-browser-use/src/mcp_server_browser_use",
                "run",
                "mcp-server-browser-use"
            ],
            "env": {
                "MCP_MODEL_PROVIDER": "google",
                "GOOGLE_API_KEY": "GOOGLE_API_KEY",
                "MCP_MODEL_NAME": "gemini-2.5-flash-preview-05-20",
                "BROWSER_HEADLESS": "false",
                "BROWSER_USE_LOGGING_LEVEL": "INFO",
                "PYTHONIOENCODING": "utf-8",
                "PYTHONUNBUFFERED": "1",
                "PYTHONUTF8": "1",
                "MCP_RESEARCH_MAX_ITERATIONS": "10"
            },
            "timeout_seconds": 7200,
            "disabled": true
        },
        "context7": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp@latest"
            ],
            "disabled": true
        }
    }
}